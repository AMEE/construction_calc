<%= update_page do |page|
    page.remove "content"
    page.insert_html :append, "types-content-area", :partial => "form"
end %>
showTabBorder("commuting");

units = new Array();
<% Commute::TYPE.keys.each do |key| %>
    units['<%= key.to_s %>'] = '<%= options_for_select(Commute::TYPE[key].unit_options).gsub("\n", "") %>';
<% end %>

updateUnits('commute');
<% if @commute.units %>
    selectUnits('commute', '<%= @commute.units %>');
<% end %>